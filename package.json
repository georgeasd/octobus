{
  "name": "octobus.js",
  "version": "0.14.3",
  "description": "An event dispatcher aimed for AOP.",
  "main": "dist/index.js",
  "scripts": {
    "test:watch": "npm t -- -w",
    "test": "mocha --compilers js:babel-register --require ./test/helper.js",
    "cover": "nyc --reporter=lcov --reporter=text-lcov npm t",
    "prebuild": "rimraf dist",
    "build": "babel src --copy-files --out-dir dist",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viczam/octobus.git"
  },
  "keywords": [
    "AOP",
    "events",
    "event",
    "emitter",
    "microservices"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Victor Zamfir <zamfir.victor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viczam/octobus/issues"
  },
  "homepage": "https://github.com/viczam/octobus#readme",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "joi": "^8.0.5",
    "lodash": "^4.11.1",
    "shortid": "2.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "5.0.0",
    "babel-plugin-transform-async-to-generator": "6.7.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.0",
    "istanbul": "0.4.3",
    "mocha": "^2.4.5",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.3",
    "sinon-chai": "2.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "transform-runtime"
    ]
  }
}
